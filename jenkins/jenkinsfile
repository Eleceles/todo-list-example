pipeline {
    agent any
    stages {
        stage('setup') {
            steps {
                sh "jenkins/setup.sh"
                
            }
        }
        stage('Test') {
            steps {
                sh "jenkins/test.sh"
                
            }
        }
        stage('Build') {
            steps {
                sh "jenkins/buid.sh"
                
            }
        }
        stage('Push') {
            steps {
                sh "jenkins/push.sh"
                
            }
        }
        stage('Deploy') {
            steps {
                sh "jenkins/deploy.sh"
                
            }
        }
    }
}